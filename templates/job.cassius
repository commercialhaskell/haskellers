h1
    font-size: 35px
    margin-left: 10px
    font-weight: normal
    text-transform: uppercase
table
    border-collapse: collapse
    margin: 0 0 0 20px
th, td
    padding: 3px
    text-align: left
th
    color: #DBEDFF
    font-weight: normal
    font-style: italic
td
    font-weight: bold
#desc
    margin: 10px 20px
    padding: 3px
    background-color: white
    p
        line-height: 1.4em
    ul
        padding-left: 40px
    li
        list-style: disc
